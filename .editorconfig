# Further information can be found here: http://editorconfig.org

root = true

[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[**.coffee]
indent_style = tab
indent_size = 4

[**.less]
indent_style = tab
indent_size = 4

[**.jade]
indent_style = tab
indent_size = 4

[**.md]
indent_style = tab
indent_size = 4
